pipeline {
  agent none
    stages( ) {
      stage('stage1') {
        agent { label 'slave1'}
           steps {
            sh '''
              echo "run on slave1"
               '''
      stage('stage2') {
        agent { agent 'master'}
          steps {
           sh '''
              echo "run on master"
               '''
} 
}    
}
}
}
}
